{
  "name": "socsel-meeting-helper",
  "displayName": "SocSEL Meeting Helper",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Daiki Maekawa. <daaikii@outlook.jp>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "format": "prettier -w src"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.9.0",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.16",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.0",
    "postcss": "^8.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "tabGroups"
    ]
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": "last 2 versions, not dead, > 0.2%"
      }
    }
  }  
}
